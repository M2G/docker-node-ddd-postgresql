{
  "name": "docker-node-ddd-mysql-typescript",
  "version": "0.0.1",
  "description": "Docker, NodeJS, MySQL, Typescript with Domain Driven Design",
  "main": "src/index.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/M2G/docker-node-ddd-mysql-typescript"
  },
  "homepage": "https://github.com/M2G/docker-node-ddd-mysql-typescript",
  "bugs": {
    "url": "https://github.com/docker-node-ddd-mysql-typescript/issues"
  },
  "keywords": [
    "docker",
    "nodejs",
    "typescript",
    "mysql",
    "ddd",
    "api"
  ],
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node --harmony-top-level-await build/src/index.js",
    "watch-node": "nodemon build/src/index.js",
    "watch": "NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/src/index.js",
    "test": "jest unit --forceExit --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "M2G",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.10",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.0.27",
    "@types/ramda": "^0.27.40",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "assert": "^1.4.1",
    "await-to-ts": "^1.0.6",
    "awilix": "^4.3.3",
    "bcrypt": "^5.0.1",
    "body-parser": "1.18.3",
    "concurrently": "^6.0.2",
    "cors": "2.8.5",
    "eslint": "7.25.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "4.16.4",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "http-status": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.4.0",
    "jest-cli": "^26.4.0",
    "jest-resolve": "26.4.0",
    "jest-watch-typeahead": "0.6.0",
    "jsonwebtoken": "8.5.0",
    "mocha": "^8.1.3",
    "morgan": "1.9.1",
    "nodemon": "1.18.10",
    "pg": "^7.8.1",
    "prettier": "^2.1.2",
    "ramda": "0.26.1",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "structure": "^2.0.1",
    "ts-jest": "^26.2.0",
    "typescript": "^4.2.4",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.11.2",
    "babel-eslint": "^10.0.3",
    "chai": "4.2.0",
    "concurrently": "^6.0.2",
    "jest": "^26.4.0",
    "mocha": "8.3.2",
    "nodemon": "^1.13.0",
    "sinon": "7.2.7",
    "sinon-chai": "3.3.0",
    "supertest": "3.4.2",
    "ts-node": "^5.0.0"
  }
}
